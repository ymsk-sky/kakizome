<h1>お題決め</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@picture) do |f| %>
      <%= f.label :theme %>
      <%= f.text_field :theme %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "筆をとる", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
